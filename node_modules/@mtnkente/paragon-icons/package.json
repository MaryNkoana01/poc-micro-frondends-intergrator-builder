{
  "name": "@mtnkente/paragon-icons",
  "version": "0.6.2",
  "license": "MIT",
  "description": "SVG-based React icon components for the Paragon design system",
  "scripts": {
    "clean": "npx rimraf ./assets/ & npx rimraf ./src/compiled-icons & npx rimraf ./src/compiled-logos",
    "figma-export": "npx figma-export use-config figma.config.js",
    "svgr-icons": "npx svgr --icon '1rem' --title-prop --typescript --replace-attr-values '#000=currentColor' -d src/compiled-icons assets/icons",
    "svgr-logos": "npx svgr --icon '5rem' --title-prop --typescript --replace-attr-values '#000=currentColor' -d src/compiled-logos assets/logos",
    "compile": "yarn run clean && yarn run figma-export && yarn run svgr-icons && yarn run svgr-logos"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "directory": "libs/icons",
    "url": "git+https://github.com/mtnkente/paragon.git"
  },
  "module": "./dist/index.esm.js",
  "main": "./dist/index.esm.js"
}
